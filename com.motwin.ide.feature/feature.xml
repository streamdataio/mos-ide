         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.motwin.ide.product"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.motwin.ide.intro"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

