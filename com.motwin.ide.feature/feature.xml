      <import plugin="org.eclipse.ui.ide" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.filesystem" version="1.3.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.ui" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="org.slf4j.api"/>
