      version="3.3.1.RELEASE"
      provider-name="Motwin"
      plugin="com.motwin.ide.product"
      exclusive="true">

   <description url="%motwinURL">
      %description
   </description>

   <copyright url="%motwinURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="%updateSiteURL"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.8.0" match="greaterOrEqual"/>
      <import plugin="com.google.guava" version="10.0.0" match="greaterOrEqual"/>
      <import plugin="org.apache.commons.io" version="2.0.0" match="greaterOrEqual"/>
